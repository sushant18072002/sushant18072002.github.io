{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch React App",
            "url": "http://localhost:3001",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "Start Dev Server",
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/src/*"
            }
        }
    ]
}